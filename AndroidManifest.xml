<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kulturhusetpalladium.palladium"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
 <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.kulturhusetpalladium.palladium.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.kulturhusetpalladium.palladium.Menulist"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MENULIST" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
       <activity
            android:name="com.kulturhusetpalladium.palladium.Gallery"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.GALLERY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter></activity>
             <activity
            android:name="com.kulturhusetpalladium.palladium.Pagecontroller"
            android:label="@string/app_name" >
            <intent-filter>
                 
                <action android:name="android.intent.action.PAGECONTROLLER" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
